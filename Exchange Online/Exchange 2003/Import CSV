
$DistroCSV = Import-Csv C:\Temp\O365_DISTRO_GROUP.csv

ForEach ($DistroGroup in $DistroCSV.DistributionGroupName) {

$DistroGroupMembers = Import-CSV .\$DistroGroup.csv

    ForEach ($DistroGroupMember in $DistroGroupMembers){
      add-DistributionGroupMember -Identity $DistroGroup -Member $DistroGroupMember.Name
      }
  }
